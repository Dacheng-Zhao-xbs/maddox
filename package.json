{
  "name": "maddox",
  "version": "2.6.0",
  "description": "Maddox provides a simple way to rapidly unit test complex success and failure scenarios which can be driven by responses of mocked external dependencies.",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run ut && npm run lint",
    "perf": "maddox -Pp -d ./spec/unit",
    "ut": "_mocha ./spec/unit",
    "coverage": "./bin/maddox-cov.js",
    "lint": "eslint ./spec/ ./lib",
    "build-docs": "node ./generate-docs/build-read-io-docs"
  },
  "bin": {
    "maddox": "./bin/maddox.js"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corybill/maddox.git"
  },
  "keywords": [
    "StriveNine",
    "Strive9",
    "S9",
    "Scenario Testing",
    "BDD",
    "Behavior Driven Development",
    "Business Requirement Driven Development",
    "BRDD",
    "BDD Unit Testing",
    "BDD Integration Testing",
    "Mocha"
  ],
  "author": "Cory Parrish",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/corybill/maddox/issues"
  },
  "homepage": "https://github.com/corybill/maddox#readme",
  "contributors": [
    {
      "name": "Kuntal Roy"
    },
    {
      "name": "Meir Rosner"
    },
    {
      "name": "Daniel Kim"
    }
  ],
  "dependencies": {
    "argparse": "1.x",
    "bluebird": "3.x",
    "chai": "3.x",
    "core-util-is": "^1.0.2",
    "errr": "2.x",
    "fs-extra": "0.x",
    "lodash": "4.x",
    "mocha": "3.x",
    "preconditions": "2.x",
    "simple-statistics": "^4.1.0",
    "sinon": "1.x"
  },
  "devDependencies": {
    "chance": "1.0.1",
    "eslint": "2.3.0",
    "fs": "0.0.2",
    "istanbul": "0.4.2",
    "node-uuid": "1.4.7",
    "semver": "5.1.0"
  }
}
